CREATE TABLE Notes (
 id TEXT NOT NULL PRIMARY KEY,
 userId TEXT NOT NULL,
 categoryId TEXT,
 title TEXT,
 description TEXT,
 createdAt TEXT,
 updatedAt TEXT
);

insertOrUpdateNote:
INSERT OR REPLACE INTO Notes(id, userId, categoryId, title, description, createdAt,updatedAt)
VALUES(?,?,?,?,?,?,?);

selectAll:
SELECT * FROM Notes;

deleteById:
DELETE FROM Notes WHERE  id = ?;

